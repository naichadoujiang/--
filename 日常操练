//1、调试下列程序，使之具有如下功能：输入a、b、c三个整数，求最小值并验证结果。
#include <stdio.h>
int main() {

#include <stdio.h>
	
		int a, b, c;
		scanf("%d%d%d", &a, &b,&c);
		if ((a > b) && (a > c))
		{
			if (b < c)
				printf("min=%d\n", b);
			else
				printf("min=%d\n", c);
		}
		else if ((a < b) && (a < c))
			printf("min=%d\n", a);



	return 0;
}
